{
    "compilerOptions": {
        "target": "ES2021",
        "module": "CommonJS",
        "lib": [
            "ES2021"
        ],
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "baseUrl": "."
    }
}